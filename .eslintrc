{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-bitwise": 1,
    "indent": [2, 4, {"SwitchCase": 1}],
    "max-len": [2, 120],
    "no-param-reassign": 0,
    "arrow-parens": ["error", "as-needed"],
    "import/imports-first": 0,
    "import/no-extraneous-dependencies": 0,
    "no-lonely-if": 0
  }
}